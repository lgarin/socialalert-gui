                   xmlns:conv="using:Socialalert.Converters"
                   xmlns:prism="using:Microsoft.Practices.Prism.StoreApps">
	<prism:MvvmAppBase.Resources>
        <!-- Application-specific resources -->
        <x:String x:Key="AppName">Social Alert</x:String>
